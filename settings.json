  "vim_mode": true,
  "vim": {
    "use_multiline_find": true,
    "use_smartcase_find": true
  },
  "lsp": {
    "jdtls": {
      "initialization_options": {
        "settings": {
          "java": {
            "configuration": {
              "runtimes": [
                {
                  "name": "JavaSE-17",
                  "path": "/Library/Java/JavaVirtualMachines/temurin-17.jdk/Contents/Home",
                  "default": true
                }
              ]
            },
            "signatureHelp": {
              "enabled": true
            },
            "maven": {
              "downloadSources": true
            },
            "inlayhints": {
              "parameterNames": {
                "enabled": "all"
              }
            },
            "jdt": {
              "ls": {
                "lombokSupport": {
                  "enabled": true
                }
              }
            }
          }
        }
      }
    }
  },
  "languages": {
    "Java": {
      "formatter": {
        "external": {
          "command": "java",
          "arguments": [
            "-jar",
            "/Users/fpetros/.local/share/nvim/google-java-format/google-java-format.jar",
            "-"
          ]
        }
      }
    }
